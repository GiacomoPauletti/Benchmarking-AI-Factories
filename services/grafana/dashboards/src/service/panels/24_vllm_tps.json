{
  "datasource": {
    "type": "prometheus",
    "uid": "prom"
  },
  "title": "Tokens Per Second (TPS)",
  "type": "timeseries",
  "id": 104,
  "gridPos": {
    "h": 8,
    "w": 12,
    "x": 12,
    "y": 14
  },
  "targets": [
    {
      "expr": "rate(vllm:generation_tokens_total{service_id=\"$service\", replica_id=~\"$replicas\"}[$__interval])",
      "legendFormat": "Generation TPS - {{replica_id}}",
      "refId": "A"
    },
    {
      "expr": "rate(vllm:prompt_tokens_total{service_id=\"$service\", replica_id=~\"$replicas\"}[$__interval])",
      "legendFormat": "Prompt TPS - {{replica_id}}",
      "refId": "B"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "tps"
    }
  }
}