{
  "datasource": {
    "type": "prometheus",
    "uid": "prom"
  },
  "title": "E2E Latency Over Time",
  "type": "timeseries",
  "id": 101,
  "gridPos": {
    "h": 8,
    "w": 12,
    "x": 0,
    "y": 6
  },
  "targets": [
    {
      "expr": "rate(vllm:e2e_request_latency_seconds_sum{service_id=\"$service\", replica_id=~\"$replicas\"}[$__interval]) / rate(vllm:e2e_request_latency_seconds_count{service_id=\"$service\", replica_id=~\"$replicas\"}[$__interval])",
      "legendFormat": "Avg E2E Latency - {{replica_id}}",
      "refId": "A"
    },
    {
      "expr": "histogram_quantile(0.99, sum by(le, replica_id) (rate(vllm:e2e_request_latency_seconds_bucket{service_id=\"$service\", replica_id=~\"$replicas\"}[$__interval])))",
      "legendFormat": "P99 E2E Latency - {{replica_id}}",
      "refId": "B"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "s"
    }
  }
}